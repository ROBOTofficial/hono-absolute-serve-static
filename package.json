{
	"name": "hono-absolute-serve-static",
	"description": "Absolute path version of @hono/node-server.",
	"main": "dist/index.cjs",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"type": "module",
	"version": "1.0.0",
	"license": "MIT",
	"homepage": "https://github.com/ROBOTofficial/hono-absolute-serve-static",
	"scripts": {
		"start": "bun run src/index.ts",
		"test": "jest",
		"build": "tsup",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"check": "tsc --noEmit",
		"changesets": "npx @changesets/cli",
		"prepublishOnly": "bun run build"
	},
	"keywords": [
		"hono",
		"absolute path",
		"serveStatic",
		"@hono/node-server"
	],
	"files": [
		"dist"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/ROBOTofficial/hono-absolute-serve-static.git"
	},
	"publishConfig": {
		"registry": "https://registry.npmjs.org",
		"access": "public"
	},
	"devDependencies": {
		"@types/bun": "latest"
	},
	"peerDependencies": {
		"typescript": "^5"
	},
	"dependencies": {
		"@changesets/cli": "^2.29.4",
		"@eslint/compat": "^1.2.9",
		"@eslint/js": "^9.26.0",
		"@hono/node-server": "^1.14.4",
		"@jest/globals": "^30.0.0",
		"@jest/types": "^30.0.0",
		"@types/jest": "^30.0.0",
		"@types/node": "^24.0.1",
		"@types/supertest": "^6.0.3",
		"bun": "^1.2.13",
		"eslint": "^9.26.0",
		"eslint-config-prettier": "^10.1.5",
		"globals": "^16.1.0",
		"hono": "^4.8.3",
		"jest": "^30.0.0",
		"prettier": "^3.5.3",
		"supertest": "^7.1.1",
		"ts-jest": "^29.4.0",
		"tsup": "^8.5.0",
		"typescript-eslint": "^8.32.0"
	},
	"engines": {
		"node": ">=18.20.8"
	}
}
